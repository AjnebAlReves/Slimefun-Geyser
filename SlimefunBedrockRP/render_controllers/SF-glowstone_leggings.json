{
	"format_version": "1.8.0",
	"render_controllers": {
		"controller.render.glowstone_leggings": {
			"geometry": "query.is_item_name_any('slot.armor.legs','1','geyser_custom:glowstone_leggings') ? Geometry.default : Geometry.none",
			"materials": [
				{
					"*": "variable.is_enchanted ? Material.enchanted : Material.default"
				}
			],
			"textures": [
				"Texture.default",
				"Texture.enchanted"
			]
		}
	}
}